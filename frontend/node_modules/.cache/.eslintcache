ssages\\StoreMessage.jsx",[],[],"C:\\Users\\giova\\Videos\\PROGRAMMING\\PROGRAMMING CAMPUS\\REACT-PROJECTS\\venezza-store\\frontend\\src\\components\\StoreMessages\\StoreLoader.jsx",[],[],"C:\\Users\\giova\\Videos\\PROGRAMMING\\PROGRAMMING CAMPUS\\REACT-PROJECTS\\venezza-store\\frontend\\src\\reducers\\cartReducers.js",["181"],[],"import { CART_ADD_ITEM, CART_REMOVE_ITEM } from \"../constants/cartConstants\";\r\n\r\nexport const cartReducer = (state = { cartItems: [] }, action) => {\r\n  switch (action.type) {\r\n    case CART_ADD_ITEM:\r\n      const item = action.payload;\r\n      const hasItem = state.cartItems.find((x) => x.product === item.product);\r\n      if (hasItem) {\r\n        return {\r\n          ...state,\r\n          cartItems: state.cartItems.map((y) =>\r\n            y.product === hasItem.product ? item : y\r\n          ),\r\n        };\r\n      } else {\r\n        return {\r\n          ...state,\r\n          cartItems: [...state.cartItems, item],\r\n        };\r\n      }\r\n    default:\r\n      return state;\r\n  }\r\n};\r\n","C:\\Users\\giova\\Videos\\PROGRAMMING\\PROGRAMMING CAMPUS\\REACT-PROJECTS\\venezza-store\\frontend\\src\\constants\\cartConstants.js",[],[],"C:\\Users\\giova\\Videos\\PROGRAMMING\\PROGRAMMING CAMPUS\\REACT-PROJECTS\\venezza-store\\frontend\\src\\actions\\cartActions.js",[],[],"C:\\Users\\giova\\Videos\\PROGRAMMING\\PROGRAMMING CAMPUS\\REACT-PROJECTS\\venezza-store\\frontend\\src\\screens\\StoreCartpage\\StoreCartpage.jsx",["182","183"],[],"C:\\Users\\giova\\Videos\\PROGRAMMING\\PROGRAMMING CAMPUS\\REACT-PROJECTS\\venezza-store\\frontend\\src\\screens\\StoreProductpage\\StoreProductpage.jsx",["184"],[],"import React, { useState, useEffect } from \"react\";\r\nimport { Link, useParams, useNavigate  } from \"react-router-dom\";\r\nimport { useDispatch, useSelector }  from 'react-redux';\r\nimport { listProductDetails } from \"../../actions/productActions\"; \r\nimport StoreSocialMedia from \"../../components/common/StoreSocialMedia\";\r\n\r\nimport \"./store_productpage.css\";\r\nimport StoreLoader from \"../../components/StoreMessages/StoreLoader\";\r\nimport StoreMessage from \"../../components/StoreMessages/StoreMessage\";\r\nimport StoreGoback from \"../../components/common/StoreGoback/StoreGoback\";\r\n\r\n\r\n\r\nconst StoreProductpage = () => {\r\n const [qty, setQty] = useState(1); \r\n  const params = useParams();  \r\n  const navigate = useNavigate();\r\n  const dispatch = useDispatch();\r\n\r\n  const productDetails = useSelector(state => state.productDetails);\r\n  const { loading, error, product} = productDetails;\r\n\r\n  useEffect(() => {\r\n    dispatch(listProductDetails(params.id)); \r\n  }, [dispatch,params]); \r\n\r\n  useEffect(() => {\r\n    window.scrollTo(0, 0);\r\n  }, []);\r\n\r\n  const allProductInStock = [...Array(product.countInStock).keys()];\r\n\r\n  const handleAddToCart = () => { \r\n    navigate(`/cart/${params.id}?qty=${qty}`); \r\n  }\r\n \r\n\r\n  return (\r\n    <main className=\"store__container\">\r\n       <div className=\"product-details store_minheight--positon\">\r\n         <StoreGoback/> \r\n         \r\n          <div className=\"product-details__wrapper\">\r\n            {loading ? (<StoreLoader/>) : \r\n              error ? ( <StoreMessage variant={'danger'}>{error}</StoreMessage>\r\n            ) : ( \r\n             <div className=\"product-content\">\r\n                <figure className=\"product-content__image\">\r\n                    <img className=\"product-content__image--item\"  src={product.image} \r\n                      alt={product.name} title={product.name} \r\n                    />\r\n                </figure>\r\n\r\n                <article className=\"product-content__description\">\r\n                    <div className=\"product-content__description--details\">\r\n                        <h1 className=\"product-content__details--title\">\r\n                          {product.name}\r\n                        </h1>\r\n                        <h2 className=\"product-content__details--price\">\r\n                            R$ {product.price}\r\n                        </h2>\r\n                    </div> \r\n                    <span></span> \r\n                    <p className=\"product-content__description--text\">\r\n                      {product.description}\r\n                    </p>\r\n                    {product.countInStock > 0 ? (\r\n                      <div className=\"product-content__description--container\">\r\n                          <form className=\"product-content__description--form\" value={qty} onChange={(e)=> setQty(e.target.value)}>\r\n                              <select className=\"product-content__form--select\">\r\n                                  <option value=\"Selecione\">Selecione</option> \r\n                                  {allProductInStock.map(q => (\r\n                                  <option key={q + 1} value={q + 1}>{q + 1}</option>       \r\n                                  ))} \r\n                              </select>\r\n                          </form> \r\n                          <button className=\"store__button-one\" onClick={handleAddToCart} type=\"button\">\r\n                            Adicionar ao carrinho\r\n                          </button> \r\n                      </div>\r\n                    ) : (\r\n                      <button className=\"store__disable-button\" disabled>\r\n                        Produto Esgotado\r\n                      </button>\r\n                    )} \r\n                  <span></span>\r\n                    <StoreSocialMedia />\r\n                </article>\r\n             </div> \r\n            )}\r\n          </div>\r\n       </div>\r\n    </main>\r\n  );\r\n};\r\n\r\nexport default StoreProductpage;\r\n","C:\\Users\\giova\\Videos\\PROGRAMMING\\PROGRAMMING CAMPUS\\REACT-PROJECTS\\venezza-store\\frontend\\src\\components\\common\\StoreGoback\\StoreGoback.jsx",[],[],{"ruleId":"185","